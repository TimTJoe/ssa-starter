{
  "name": "ssa-starter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon -e js,json,env index.js",
    "generate:migration": "npx drizzle-kit generate",
    "apply:migration": "npx drizzle-kit migrate",
    "up:migration": "npx drizzle-kit up",
    "check:migration": "npx drizzle-kit check",
    "down:migration": "npx drizzle-kit down",
    "status:migration": "npx drizzle-kit status"
  },
  "keywords": [],
  "author": "timothy t. joe",
  "license": "ISC",
  "description": "ssa-starter files with drizzle-orm and node-express",
  "dependencies": {
    "@libsql/client": "^0.14.0",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.28.1",
    "drizzle-orm": "^0.36.4",
    "express": "^4.21.1",
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  },
  "_moduleAliases": {
    "@root": ".",
    "@controller": "controller",
    "@database": "db",
    "@migration": "migration",
    "@route": "route",
    "@schema": "schema",
    "@middleware": "middleware",
